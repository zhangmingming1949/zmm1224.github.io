	    function drawLine4(ec){
	    　　var myLineChart = ec.init(document.getElementById('lineMain4'));
	        var option4 = {
	            title: {
	                text: '截止31日回合同占比'
	            },
	            tooltip: {
	                trigger: 'axis',
	                axisPointer: {
	                    type: 'cross',
	                    crossStyle: {
	                        color: '#999'
	                    }
	                }
	            },
	            toolbox: {
	                feature: {
	                    dataView: {show: true, readOnly: false},
	                    magicType: {show: true, type: ['line', 'bar']},
