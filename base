	<script src="http://echarts.baidu.com/build/dist/echarts.js"></script>
	<script type="text/javascript">
		// 路径配置
	    require.config({
	    　　paths: {
	    　　　　echarts: 'http://echarts.baidu.com/build/dist'
	    　　}
	    });
	// 使用
	    require(
	    　　　　　　[
	    　　　　　　　　'echarts',
	    				'echarts/chart/pie',
	    　　　　　　　　'echarts/chart/bar',
	    　　　　　　　　'echarts/chart/line'
	    　　　　　　],
	    　　　　　　drawEcharts
	    );
	 
	    function drawEcharts(ec){
	    　　drawLine(ec);
	        drawLine2(ec);
	        drawLine3(ec);
	        drawLine4(ec);
	        drawLine5(ec);
	    }
	   
	 
	    function drawLine(ec){
	    　　var myLineChart = ec.init(document.getElementById('lineMain'));

	    	var data1=[100, 0, 0, 0, 0]
	    	var data2=[100, 100, 701, 734]

	        option = {
			    tooltip : {
			        trigger: 'axis',
			        axisPointer : {            // 坐标轴指示器，坐标轴触发有效
			            type : 'shadow'        // 默认为直线，可选为：'line' | 'shadow'
			        }
			    },
			    legend: {
			        data:['爱普优邦','小鱼赚钱','蜜蜂试玩','钱派试玩','易赚','凡客','钱咖','热葫芦','米淘赚','易米','天天钱庄','小啄赚钱']
			    },
			    grid: {
			        left: '3%',
			        right: '4%',
			        bottom: '3%',
			        containLabel: true
			    },
			    xAxis : [
			        {
			            type : 'category',
			            data : ['京东商城','小黑鱼','DADA英语','优信二手车']
			        }
			    ],
			    yAxis : [
			        {
			            type : 'value'
			        }
			    ],
			    series : [	

			        {
			            name:'小啄赚钱',
			            type:'bar',
			            stack: '三',
			            data:data1,
			            label:{
	            			show:true,
	            			position:'top',
	            			formatter:function(params){
	            				if(params.value>0){
	            					return params.value;
	            				}else{
	            					return'';
	            				}
	            			}
	            		}
			        },
			        {
			            name:'天天钱庄',
			            type:'bar',
			            stack: '三',
			            data:data2
			        },
			        {
			            name:'易米',
			            type:'bar',
			            stack: '二',
			            data:[0, 400, 701, 734]
			        },
			        {
			            name:'米淘赚',
			            stack: '二',
			            type:'bar',
			            data:[1000, 0, 964, 1026]
			        },
			         {
			            name:'热葫芦',
			            type:'bar',
			            stack: '四',
			            data:[]
			        },
			        {
			            name:'钱咖',
			            type:'bar',
			            stack: '一',
			            data:['-', 700, 964, 1026, 1679]
			        },
			        {
			            name:'凡客',
			            type:'bar',
			            stack: '一',
			            data:[0, 0, 201, 154, 190, 330]
			        },
			        {
			            name:'易赚',
			            type:'bar',
			            stack: '二',
			            data:[400, 0, 101, 134, 90, 230]
			        },
			        {
			            name:'钱派试玩',
			            type:'bar',
			            stack: '一',
			            data:[0, 1300, 191, 234, 290]
			        },
			        {
			            name:'蜜蜂试玩',
			            type:'bar',
			            stack: '一',
			            data:[700, 0, 91, 84, 109, 110]
			        },
			        {
			            name:'小鱼赚钱',
			            type:'bar',
			            stack: '一',
			            data:[800, 500, 101, 134, 90]
			        }, 
			        {
			            name:'爱普优邦',
			            type:'bar',
			            stack: '一',
			            data:[1000, 900, 301, 334, 390]
			        }
			    ]
			};

	        myLineChart.setOption(option,true);
	    }


	    function drawLine2(ec){
	    　　var myLineChart = ec.init(document.getElementById('lineMain2'));

	        var option2 = {
	            title: {
	                text: '截止31日回合同占比'
	            },
	            tooltip : {
	                trigger: 'axis',
	                axisPointer: {
	                    type: 'cross',
	                    label: {
	                        backgroundColor: '#6a7985'
	                    }
	                }
	            },
	            legend: {
	                data:['回合同占比']
	            },
	            toolbox: {
	                feature: {
	                    saveAsImage: {}
	                }
	            },
	            grid: {
	                left: '3%',
	                right: '4%',
	                bottom: '3%',
	                containLabel: true
	            },
	            xAxis : [
	                {
	                    type : 'category',
	                    boundaryGap : false,
	                    data : ['1周','2周','3周','4周','5周']
	                }
	            ],
	            yAxis : [
	                {
	                    type: 'value',
	                    name: '占比',
	                    min: 0,
	                    max: 100,
	                    interval: 50,
	                    axisLabel: {
	                        formatter: '{value} %'
	                    }
	                }
	            ],
	            series : [
	                {
	                    name:'回合同占比',
	                    type:'line',
	                    stack: '总量',
	                    areaStyle: {normal: {}},
	                    data:[90, 87, 70, 65, 59]
	                },
	            ]
	        };
	        myLineChart.setOption(option2,true);
	    }

